.MODEL SMALL
.STACK 100H
.DATA 
    TEXT DB "UPPER LETTERS ARE: $"
    TEXT2 DB 10,13,"LOWER LETTERS ARE: $"
.CODE
MAIN PROC
    MOV AX,@DATA
    MOV DS,AX 
    MOV AH,09
    LEA DX,TEXT
    INT 21H
    MOV CL,26 ;UPPERCASE
    MOV DL,65

    LOOP1:
    MOV AH,02
    INT 21H 
    INC DL
    DEC CL 
    MOV BL,DL
    MOV DL,32
    INT 21H
    MOV DL,BL
    JNZ LOOP1
     
    MOV AH,09
    LEA DX,TEXT2
    INT 21H
    MOV CL,26    ;LOWERCASE
    MOV DL,97 
    LOOP2:
    MOV AH,02
    INT 21H
    INC DL
    DEC CL 
    MOV BL,DL
    MOV DL,32
    INT 21H
    MOV DL,BL
    JNZ LOOP2
    
    MOV AH,4CH
    INT 21H
    MAIN ENDP
END MAIN
